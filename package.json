  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development --config ./config/vite.config.dev.ts -- --Folder",
    "dev:prod": "vite --mode production --config ./config/vite.config.dev.ts -- --Folder",
    "build:test": "vite build --mode development  --config  ./config/vite.config.prod.ts -- --Folder",
    "build:prod": "vite build --mode production  --config ./config/vite.config.prod.ts -- --Folder",
    "preview": "vite preview --config ./config/vite.config.prod.ts -- --Folder",
